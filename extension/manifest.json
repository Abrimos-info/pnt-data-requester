{
    "manifest_version": 3,
    "name": "pnt-data-requester",
    "version": "1.0.0",
    "description": "Aumenta la funcionalidad de la Plataforma Nacional de Transparencia (PNT) de MÃ©xico",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [ "https://www.plataformadetransparencia.org.mx/*" /*, "https://servicios.plataformadetransparencia.org.mx/*"*/ ],
            "js": [ "scripts/thirdParty/jquery-3.6.1.min.js", "scripts/content.js" ]

        }
    ],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": ["activeTab", "tabs"],
    "version_name": "v1.0.0",
    "background": {
        "service_worker": "background.js"
    }
}
